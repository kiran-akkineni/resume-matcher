{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/compute_similarity.py",
        "use": "@vercel/python",
        "config": {
          "postInstall": "python post_install.py"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/compute_similarity",
        "dest": "/api/compute_similarity.py"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  